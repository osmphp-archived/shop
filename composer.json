{
    "name": "osmphp/shop",
    "description": "Osm Shop is an e-commerce application that you can install on your server and use.",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Vladislav Osmianskij",
            "email": "vlaosm@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "osmphp/framework": "^0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Osm\\Shop\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Osm\\Shop\\Tests\\": "tests/",
            "Osm\\Shop\\Samples\\": "samples/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/osmphp/core"
        },
        {
            "type": "vcs",
            "url": "https://github.com/osmphp/framework"
        }
    ]
}
